{
    "name": "mars-lib/scaffold",
    "license": "MIT",
    "description": "a php simple scaffold",
    "authors": [
        {
            "name": "MarsLib",
            "email": "marslibss@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "ext-pdo": "*",
        "ext-mongodb": "*",
        "ext-redis": "*",
        "ext-json": "*",
        "mongodb/mongodb": "^1.4",
        "seld/jsonlint": "^1.7",
        "wikia/prometheus_client_php": "^0.9.2"
    },
    "autoload": {
        "psr-4": {
            "MarsLib\\Scaffold\\": "src/"
        },
        "files": [
            "helpers/common.php",
            "helpers/array.php",
            "helpers/string.php",
            "helpers/parameter.php",
            "helpers/system.php"
        ]
    },
    "require-dev": {
        "symfony/var-dumper": "^4.1"
